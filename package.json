  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "npm-run-all --parallel watch:client watch:server",
    "watch:client": "webpack serve --mode=development --config config/webpack.dev.js --hot",
    "watch:server": "nodemon --inspect server/index.js",
    "build": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build-staging": "NODE_ENV=production webpack --config config/webpack.stage.js",
    "prettify": "yarn prettier \"**/*.*(js|jsx)\" --ignore-path=.prettierignore --write",
    "lint": "eslint src --ext .js,.jsx && yarn prettify",
    "lint-fix": "eslint src --ext .js,.jsx --fix && yarn prettify",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged --verbose"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
